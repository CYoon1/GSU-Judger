<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Group6"
        tools:targetApi="31">
        <activity
            android:name=".MainActivity"
            android:exported="false"
            android:label="@string/app_name"
            android:theme="@style/Theme.Group6">
        </activity>

        <activity
            android:name=".EventListener"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.AppCompat">
        </activity>

        <activity
            android:name=".Event"
            android:exported="false"
            android:label="@string/app_name"
            android:theme="@style/Theme.Group6">
        </activity>

        <activity
            android:name=".ShowEvent"
            android:exported="false"
            android:label="@string/app_name"
            android:theme="@style/Theme.Group6">
        </activity>

        <activity
            android:name=".EventAdapter"
            android:exported="false"
            android:label="@string/app_name"
            android:theme="@style/Theme.Group6">
        </activity>

        <activity
            android:name=".AddEvent"
            android:exported="false"
            android:label="@string/app_name"
            android:theme="@style/Theme.Group6">
        </activity>

        <activity
            android:name=".AddProject"
            android:exported="false"
            android:label="@string/app_name"
            android:theme="@style/Theme.Group6">
        </activity>

        <activity
            android:name=".Project"
            android:exported="false"
            android:label="@string/app_name"
            android:theme="@style/Theme.Group6">
        </activity>

        <activity
            android:name=".ProjectAdapter"
            android:exported="false"
            android:label="@string/app_name"
            android:theme="@style/Theme.Group6">
        </activity>

        <activity
            android:name=".ShowProject"
            android:exported="false"
            android:label="@string/app_name"
            android:theme="@style/Theme.AppCompat">
        </activity>
        <activity
            android:name=".EditProject"
            android:exported="false"
            android:label="@string/app_name"
            android:theme="@style/Theme.Group6">
        </activity>
        <activity
            android:name=".Login"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.Group6">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".Registration"
            android:exported="false"
            android:label="@string/app_name"
            android:theme="@style/Theme.Group6">
        </activity>

    </application>

</manifest>